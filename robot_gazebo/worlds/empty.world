<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">

    <!-- global光源 -->
    <include>
      <uri>model://sun</uri>
      <pose>0 0 0 0.0 -0.785 0.785</pose> <!-- 约等于45°斜射 -->
    </include>
    
    <scene>
      <ambient>1 1 1 1</ambient>   <!-- 环境光亮度 -->
      <background>1 1 1 1</background> <!-- 背景亮度可选 -->
    </scene>

    <!-- 地面 -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    <include>
      <uri>model://aic_plane</uri>
      <pose>1.65 1.65 0 0 0 3.1415</pose>
    </include>

    <!-- 人物墙 -->
    <include>
      <uri>model://aic_ro1</uri>
      <pose>2.5 1.65 0 0 0 0</pose>
    </include>
    <include>
      <uri>model://aic_ro2</uri>
      <pose>2.5 0.7 0 0 0 3.1415</pose>
    </include>

    <!-- 车牌墙 -->
    <include>
      <uri>model://aic_cp</uri>
      <pose>0.1 2.7 0 0 0 1.57</pose>
    </include>

    <!-- 物理引擎参数 -->
    <physics name='default_physics' default='0' type='ode'>
      <gravity>0 0 -9.8066</gravity>
      <ode>
        <solver>
          <type>quick</type>
          <iters>10</iters>
          <sor>1.3</sor>
          <use_dynamic_moi_rescaling>0</use_dynamic_moi_rescaling>
        </solver>
        <constraints>
          <cfm>0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100</contact_max_correcting_vel>
          <contact_surface_layer>0.001</contact_surface_layer>
        </constraints>
      </ode>
      <max_step_size>0.004</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>250</real_time_update_rate>
      <magnetic_field>6.0e-6 2.3e-5 -4.2e-5</magnetic_field>
    </physics>

    <!-- 黑色材质 -->
    <material name="Black">
      <ambient>0.0 0.0 0.0 1</ambient>
      <diffuse>0.0 0.0 0.0 1</diffuse>
      <specular>0.1 0.1 0.1 1</specular>
      <emissive>0 0 0 1</emissive>
    </material>

    <!-- 围墙 -->
    <!-- 长度 4m，墙厚度 0.1m，高度 1.0m -->
    <model name="wall1">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>4 0.1 1.0</size>
            </box>
          </geometry>
          <pose>1.65 -0.35 0 0 0 0</pose>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>4 0.1 1.0</size>
            </box>
          </geometry>
          <material>
            <ambient>0.0 0.0 0.0 1</ambient>
          </material>
          <pose>1.65 -0.35 0 0 0 0</pose>
        </visual>
      </link>
    </model>

    <model name="wall2">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>4 0.1 1.0</size>
            </box>
          </geometry>
          <pose>1.65 3.65 0 0 0</pose>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>4 0.1 1.0</size>
            </box>
          </geometry>
          <material>
            <ambient>0.0 0.0 0.0 1</ambient>
          </material>
          <pose>1.65 3.65 0 0 0</pose>
        </visual>
      </link>
    </model>

    <model name="wall3">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>0.1 4 1.0</size>
            </box>
          </geometry>
          <pose>-0.35 1.65 0 0 0 0</pose>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 4 1.0</size>
            </box>
          </geometry>
          <material>
            <ambient>0.0 0.0 0.0 1</ambient>
          </material>
          <pose>-0.35 1.65 0 0 0 0</pose>
        </visual>
      </link>
    </model>

    <model name="wall4">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <box>
              <size>0.1 4 1.0</size>
            </box>
          </geometry>
          <pose>3.65 1.65 0 0 0 0</pose>
        </collision>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 4 1.0</size>
            </box>
          </geometry>
          <material>
            <ambient>0.0 0.0 0.0 1</ambient>
          </material>
          <pose>3.65 1.65 0 0 0 0</pose>
        </visual>
      </link>
    </model>
  </world>
</sdf>
